    "isActive": false
  },
  "singleNode": {
    "isActive": true,
    "publicUrl": "http://localhost/"
  },
  "influxdb": {
    "host": "127.0.0.1", 
    "port": 8086
  },
  "custom": {
    "systemStreams": {
      "account": [
        {
          "id": "insurancenumber",
