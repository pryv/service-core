          }
        },
        {
          "id": "phoneNumber",
          "isEditable": true,
          "isShown": true,
          "type": "string/pryv"
        },
        {
          "isIndexed": true,
          "isUnique": true,
          "isShown": true,
          "isEditable": true,
          "type": "email/string",
          "name": "Email",
          "id": "email",
          "isRequiredInValidation" : true
        }
      ]
    }
