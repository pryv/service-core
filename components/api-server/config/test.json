{
  "server": {
    "hostname": "test.DOMAIN"
  },
  "auth": {
    "trustedApps": "pryv-test@http://test.pryv.local, pryv-test-no-cors@*, *@https://*.rec.la*"
  },
  "logs": {
    "console": {
      "active": false,
      "level": "info",
      "timestamp": false
    }
  },
  "serviceInfoUrl": "file://test/service-info.json",
  "services": {
    "email": {
      "enabled": false,
      "method": "mandrill",
      "url": "https://mandrillapp.local/api/1.0/messages/send-template.json"
    },
    "register": {
      "url": "http://localhost:5123",
      "key": "test-system-key"
    }
  },
  "tcpMessaging": {
    "enabled": true
  },
  "reporting": {
    "licenseName": "pryv.io-test-license",
    "templateVersion": "1.0.0"
  },
  "database": {
    "host": "localhost",
    "port": 27017,
    "name": "pryv-node-test"
  },
  "openSource": {
    "isActive": false
  },
  "singleNode": {
    "isActive": true
  },
  "systemStreams": {
    "custom": [
       {
        "id": "insurancenumber",
        "isIndexed": true,
        "isShown": true,
        "type": "string/pryv",
        "isRequiredInValidation": true,
        "regexValidation": "^[0-9]*$",
        "regexError": {
          "message": "Cool error",
          "code": "cool-error"
        }
      },
      {
        "id": "phoneNumber",
        "isEditable": true,
        "isShown": true
      }
    ]
  }
}
