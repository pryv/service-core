{
  "name": "api-server",
  "version": "1.3.0",
  "private": true,
  "main": "src/index.js",
  "scripts": {
    "start": "NODE_ENV=development node src/server",
    "test": "yarn run _test --logs:console:active=false",
    "test-detailed": "yarn run _test --reporter=spec",
    "test-debug": "yarn run _test --timeout 3600000 --reporter=spec --debug-brk",
    "test-profile": "yarn run _test --profile=true --logs:console:active=false && tick-processor > profiling-output.txt && open profiling-output.txt",
    "_test": "NODE_ENV=test ../../node_modules/.bin/mocha --timeout 10000 --reporter=dot 'test/**/*.test.js'",
    "update-event-types": "scripts/update-event-types.bash"
  },
  "dependencies": {
    "airbrake": "^2.1.0",
    "awaiting": "^3.0",
    "msgpack5": "^3.6",
    "multistream": "^2.1.0",
    "nats": "^0.7",
    "slug": "^0.9.1",
    "socket.io": "^0.9.19",
    "url": "^0.11.0",
    "z-schema": "^3.15.3"
  }
}
