{
  "name": "pryv-service-core",
  "description": "Pryv core server app components",
  "version": "1.2.18",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git://github.com/pryv/service-core.git"
  },
  "workspaces": {
    "packages": ["components/*"]
  },
  "scripts": {
    "cover": "yarn cover:clean && yarn cover:do && yarn cover:report",
    "cover:clean": "rm -rf .nyc_output",
    "cover:do": "node scripts/components-command ../../node_modules/.bin/nyc --no-clean --temp-dir=../../../.nyc_output yarn run test",
    "cover:report": "node_modules/.bin/nyc report --reporter=html",
    "test": "node scripts/components-command yarn test",
    "database": "scripts/start-database.sh",
    "api": "cd dist/components/api-server && NODE_ENV=development bin/server",
    "local": "cd dist/components/api-server && NODE_ENV=development nodemon bin/server",
    "previews": "cd dist/components/previews-server && npm start",
    "metadata": "cd dist/components/metadata && bin/metadata",
    "webhooks": "cd dist/components/webhooks && bin/server",
    "nats": "gnatsd",
    "influx": "influxd -config /usr/local/etc/influxdb.conf",
    "setup": "yarn install --ignore-optionals",
    "jsdoc": "node_modules/.bin/jsdoc -c jsdoc.json",
    "test-root": "node_modules/.bin/mocha --compilers js:babel-register --timeout 10000 --reporter=dot test/**/*.test.js",
    "watch": "node_modules/.bin/babel --verbose --watch=./components --out-dir=dist/components --copy-files --source-maps both",
    "build-dev": "scripts/prepare_dist && node_modules/.bin/babel ./components --out-dir=dist/components --copy-files --source-maps both",
    "flow-coverage": "node_modules/.bin/flow-coverage-report -i 'components/**/*.js' -t html",
    "init-test-results-repo": "scripts/test-results/init_repo.sh",
    "tag-tests": "node scripts/test-ids.js",
    "output-test-results": "node scripts/test-results/test-results.js",
    "upload-test-results": "scripts/test-results/upload.sh",
    "release": "scripts/prepare_dist && node_modules/.bin/babel ./components --verbose --out-dir=dist/components --copy-files",
    "license": "cd licenser ; yarn install; yarn apply"
  },
  "dependencies": {
    "async": "^2.5.0",
    "awaiting": "^3.0.0",
    "axon": "2.0.x",
    "bcrypt": "^5.0.0",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.1",
    "cls-hooked": "^4.2.2",
    "cookie-parser": "^1.4.3",
    "cron": "^1.2.1",
    "cross-fetch": "^3.0.6",
    "express": "^4",
    "flow-bin": "^0.113.0",
    "heap": "^0.2.6",
    "hjson": "^3.1.1",
    "influx": "^5.0.6",
    "jaeger-client": "^3.8.0",
    "js-yaml": "^3.11.0",
    "lib-reporting": "git+ssh://git@github.com/pryv/lib-reporting",
    "lodash": "^4.17.4",
    "lru-cache": "^4.1.1",
    "mkdirp": "1.0.4",
    "mongodb": "^3.6.2",
    "morgan": "^1.8.1",
    "msgpack5": "^3.6.0",
    "multer": "^1.3.0",
    "nats": "^0.7",
    "protobufjs": "^6.8.6",
    "pryv": "^2.0.2",
    "qs": "^6.3.0",
    "ramda": "^0.23.0",
    "rimraf": "^2.4.3",
    "rsvp": "^4.8.1",
    "shimmer": "^1.2.0",
    "sinon": "^5",
    "slug": "^0.9.1",
    "superagent": "^3.8",
    "tchannel": "^3.9.12",
    "unicode": "^12.1.0",
    "unix-timestamp": "^0.2.0",
    "yargs": "^16.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-flow": "^7.9.0",
    "@babel/register": "^7.9.0",
    "babel-eslint": "^10.1.0",
    "blanket": "^1.2.3",
    "chai": "^4.1.2",
    "charlatan": "^1.1.0",
    "colors": "^1.0.3",
    "cookie": "^0.3.1",
    "cuid": "^1",
    "date-utils": "latest",
    "deep-equal": "latest",
    "eslint": "^7.0.0",
    "eslint-plugin-flowtype": "latest",
    "flow-coverage-report": "0.8.0",
    "flow-typed": "^3.2.1",
    "inspect": "^0.0.2",
    "jsdoc": "^3.4.3",
    "jsdoc-babel": "^0.5.0",
    "mocha": "^8.1.2",
    "moment": "^2.24.0",
    "nock": "9.4.0",
    "nyc": "^15.0.1",
    "parallelshell": "^2.0.0",
    "pre-commit": "^1.2.2",
    "should": "^11.2.0",
    "socket.io-client": "^2.3.0",
    "supertest": "^3.0.0",
    "temp": "^0.8.3",
    "tmp": "^0.0.33",
    "wrench": "latest"
  },
  "resolutions": {
    "**/bl": "^2.2.1",
    "**/node-fetch": "^2.6.1",
    "**/ini": "^1.3.6",
    "**/socket.io": "^2.4.0"
  },
  "engines": {
    "node": "12.x",
    "yarn": "1.x"
  },
  "pre-commit": [
    "license"
  ],
  "license": "UNLICENSED"
}
