{
  "name": "pryv-service-core",
  "version": "1.7.12",
  "private": true,
  "description": "Pryv core server app components",
  "homepage": "https://pryv.com",
  "repository": {
    "type": "git",
    "url": "git://github.com/pryv/service-core.git"
  },
  "license": "UNLICENSED",
  "author": "Pryv S.A. <info@pryv.com> (https://pryv.com)",
  "workspaces": {
    "packages": [
      "components/*"
    ]
  },
  "scripts": {
    "api": "cd dist/components/api-server && NODE_ENV=development bin/server",
    "build-dev": "scripts/prepare_dist && node_modules/.bin/babel ./components --out-dir=dist/components --copy-files --source-maps both",
    "cover": "node_modules/.bin/nyc --reporter=html --report-dir=./coverage yarn run test",
    "database": "scripts/start-database.sh",
    "flow-coverage": "node_modules/.bin/flow-coverage-report -i 'components/**/*.js' -t html",
    "influx": "influxd -config /usr/local/etc/influxdb.conf",
    "init-test-results-repo": "scripts/test-results/init_repo.sh",
    "license": "node_modules/.bin/source-licenser licensing/config.yml licensing/LICENSE.src ./",
    "local": "cd dist/components/api-server && NODE_ENV=development nodemon bin/server",
    "metadata": "cd dist/components/metadata && bin/metadata",
    "nats": "nats-server",
    "output-test-results": "node scripts/test-results/test-results.js",
    "previews": "cd dist/components/previews-server && npm start",
    "release": "scripts/prepare_dist && node_modules/.bin/babel ./components --verbose --out-dir=dist/components --copy-files",
    "setup": "yarn install --ignore-optionals",
    "tag-tests": "node scripts/test-ids.js",
    "test": "node scripts/components-command yarn test",
    "test-root": "node_modules/.bin/mocha --compilers js:babel-register --timeout 10000 --reporter=dot test/**/*.test.js",
    "upload-test-results": "scripts/test-results/upload.sh",
    "watch": "node_modules/.bin/babel --verbose --watch=./components --out-dir=dist/components --copy-files --source-maps both",
    "webhooks": "cd dist/components/webhooks && bin/server",
    "c2w": "rm -rf dist && rm -rf components/**/node_modules && yarn setup && yarn release && yarn watch",
    "tracing": "open http://localhost:16686/; docker run --rm -p 6831:6831/udp -p 6832:6832/udp -p 16686:16686 jaegertracing/all-in-one:1.7 --log-level=debug;"
  },
  "pre-commit": [
    "license"
  ],
  "resolutions": {
    "**/bl": "^2.2.1",
    "**/ini": "^1.3.6",
    "**/lodash": "^4.17.21",
    "**/node-fetch": "^2.6.1",
    "**/xmlhttprequest-ssl": "^1.6.2"
  },
  "dependencies": {
    "@pryv/boiler": "https://github.com/pryv/pryv-boiler.git#1.0.5",
    "async": "^2.6.4",
    "awaiting": "^3.0.0",
    "axon": "2.0.x",
    "bcrypt": "^5.0.0",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.1",
    "cls-hooked": "^4.2.2",
    "cookie-parser": "^1.4.3",
    "cron": "^1.2.1",
    "cross-fetch": "^3.0.6",
    "express": "^4",
    "flow-bin": "^0.113.0",
    "heap": "^0.2.6",
    "hjson": "^3.1.1",
    "influx": "^5.0.6",
    "jaeger-client": "^3.18.1",
    "js-yaml": "^3.11.0",
    "lodash": "^4.17.4",
    "lru-cache": "6.x",
    "mkdirp": "1.0.4",
    "mongodb": "^3.6.2",
    "morgan": "^1.8.1",
    "multer": "^1.3.0",
    "opentracing": "^0.14.5",
    "protobufjs": "^6.8.6",
    "pryv": "^2.0.2",
    "qs": "^6.3.0",
    "ramda": "^0.23.0",
    "rimraf": "^2.4.3",
    "rsvp": "^4.8.1",
    "shimmer": "^1.2.0",
    "sinon": "^5",
    "slug": "^0.9.1",
    "superagent": "^3.8",
    "tchannel": "^3.9.12",
    "unicode": "^12.1.0",
    "unix-timestamp": "^0.2.0",
    "xmlhttprequest-ssl": "^1.6.2",
    "yargs": "^16.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-flow": "^7.9.0",
    "@babel/register": "^7.9.0",
    "babel-eslint": "^10.1.0",
    "blanket": "^1.2.3",
    "chai": "^4.1.2",
    "charlatan": "^1.1.0",
    "colors": "^1.0.3",
    "cookie": "^0.3.1",
    "cuid": "^1",
    "date-utils": "latest",
    "deep-equal": "latest",
    "eslint": "^7.0.0",
    "eslint-plugin-flowtype": "^5.8.0",
    "flow-coverage-report": "0.8.0",
    "flow-typed": "^3.2.1",
    "inspect": "^0.0.2",
    "mocha": "^8.1.2",
    "moment": "^2.24.0",
    "nock": "13.1.1",
    "nyc": "^15.0.1",
    "parallelshell": "^2.0.0",
    "pre-commit": "^1.2.2",
    "should": "^11.2.0",
    "socket.io-client": "^3.1.3",
    "source-licenser": "1.0.4",
    "supertest": "^3.0.0",
    "temp": "^0.8.3",
    "tmp": "^0.0.33",
    "wrench": "latest"
  },
  "engines": {
    "node": "16.x",
    "yarn": "1.x"
  }
}
