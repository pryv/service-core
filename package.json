{
  "name": "pryv-service-core",
  "description": "Pryv core server app components",
  "version": "1.1.7",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git://github.com/pryv/service-core.git"
  },
  "scripts": {
    "all": "parallelshell 'npm run database' 'npm run api' 'npm run previews' 'npm run proxy'",
    "database": "scripts/start-database.sh",
    "api": "cd components/api-server && npm start",
    "previews": "cd components/previews-server && npm start",
    "proxy": "node scripts/compile-proxy-config proxy/vars.development.js && nginx -c \"`pwd`/proxy/nginx.conf\"",
    "test": "node scripts/components-npm test",
    "analysis": "plato -t 'service-core code analysis' -l .jshintrc -r -x 'node_modules|reports|\\.json' -d analysis-output components && open analysis-output/index.html",
    "check-dependencies": "node scripts/components-checkdeps",
    "update-components-version": "node scripts/components-version",
    "flow": "node_modules/.bin/flow; test $? -eq 0 -o $? -eq 2",
    "jsdoc": "node_modules/.bin/jsdoc -c jsdoc.json",
    "test-root": "node_modules/.bin/mocha --compilers js:babel-register --timeout 10000 --reporter=dot test/**/*.test.js",
    "watch": "node_modules/.bin/babel --watch=./source --out-dir=./components --copy-files",
    "release": "node_modules/.bin/babel ./source --out-dir=./components --copy-files"
  },
  "dependencies": {
    "api-server": "file:source/api-server",
    "async": "^2.1.4",
    "axon": "2.0.x",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "body-parser": "^1.17.1",
    "cookie-parser": "^1.4.3",
    "cron": "^1.2.1",
    "dependable": "attentif/dependable#0bb5c4a9ef2b32dded1b24d5d3c0ecccfb0bf7fd",
    "errors": "file:source/errors",
    "express": "^4.15.2",
    "hfs-server": "file:source/hfs-server",
    "lodash": "^4.17.4",
    "middleware": "file:source/middleware",
    "mkdirp": "0.5.x",
    "model": "file:source/model",
    "morgan": "^1.8.1",
    "previews-server": "file:source/previews-server",
    "pryvuser-cli": "file:source/pryvuser-cli",
    "qs": "^6.3.0",
    "ramda": "^0.23.0",
    "rimraf": "^2.4.3",
    "storage": "file:source/storage",
    "superagent": "^3.4.4",
    "test-helpers": "file:source/test-helpers",
    "unix-timestamp": "^0.2.0",
    "utils": "file:source/utils"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-register": "^6.23.0",
    "blanket": "^1.2.3",
    "colors": "^1.0.3",
    "cookie": "latest",
    "date-utils": "latest",
    "deep-equal": "latest",
    "eslint": "^3.16.0",
    "eslint-plugin-flowtype": "^2.30.0",
    "flow-bin": "^0.41.0",
    "jsdoc": "^3.4.3",
    "jsdoc-babel": "^0.3.0",
    "mocha": "^3.2.0",
    "nixt": "^0.5.0",
    "nock": "^9.0.2",
    "parallelshell": "^2.0.0",
    "should": "^11.2.0",
    "socket.io-client": "0.9.x",
    "supertest": "^3.0.0",
    "temp": "latest",
    "wrench": "latest"
  },
  "engines": {
    "node": "~6.9.5"
  }
}
