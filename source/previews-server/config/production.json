{
  "env": "production",
  "http": {
    "ip": "127.0.0.1",
    "port": 9001
  },
  "database": {
    "authUser": "previews-server",
    "authPassword": "OVERRIDE ME",
    "host": "localhost",
    "port": 27017,
    "name": "pryv-node"
  },
  "eventFiles": {
    "attachmentsDirPath": "${PRYV_DATAPATH}/service-core-files/attachments",
    "previewsDirPath": "${PRYV_DATAPATH}/service-core-files/previews"
  },
  "logs": {
    "console": {
      "active": true,
      "level": "info",
      "colorize": false
    },
    "file": {
      "active": true,
      "level": "error",
      "path": "${PRYV_LOGSDIR}/previews-server.errors.log",
      "maxFileBytes": 4096,
      "maxNbFiles": 20
    },
    "airbrake": {
      "active": true,
      "key": "OVERRIDE ME"
    }
  }
}
