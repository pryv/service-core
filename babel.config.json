{
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-proposal-private-methods"],
  "presets": [
    
    ["@babel/preset-env", { 
      "targets": {
        "node": "current"
      }, 
      "modules": false
    }],
    "@babel/preset-flow"
  ],
  "ignore": [
    "**/node_modules"
  ]
}
