{
  "env": "production",
  "webhooks": {
    "minIntervalMs": 5000,
    "maxRetries": 5,
  },
  "mongodb": {
    "host": "127.0.0.1",
    "port": 27017,
    "name": "pryv-node",
    "authUser": "",
    "authPassword": ""
